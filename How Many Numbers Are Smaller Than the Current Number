class Solution(object):
    def smallerNumbersThanCurrent(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        result = []
    
        for i in range(len(nums)):
            res=0
            for j in range(len(nums)):
                if nums[i]>nums[j]:
                    res +=1
            result.append(res)
        return result

                        
                    
